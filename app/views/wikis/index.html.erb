<div class="container">
  
  <%= link_to "Create Wiki", new_wiki_path, class: 'btn btn-primary' %>
  
  <h1>All Wikis</h1>
  <% @wikis.each do |wiki| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to wiki.title, wiki %>
          <small>
            <br>
            created <%= time_ago_in_words(wiki.created_at) %> ago by <%= wiki.user.email %>
          </small>
        </h4>
      </div>
    </div>
  <% end %>
  
</div>
